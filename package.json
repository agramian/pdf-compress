{
  "name": "pdf-compress",
  "version": "0.1.0",
  "description": "JavaScript PDF compressor/shrinker/reducer",
  "main": "./lib/pdf-compress.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "istanbul cover _mocha -- --require babel-core/register --require babel-polyfill --recursive",
    "lint": "eslint src test",
    "build": "webpack --env dev && webpack --env build && npm run test",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agramian/pdf-compress.git"
  },
  "keywords": [
    "pdf",
    "compress",
    "compressor",
    "shrink",
    "shrinker",
    "reduce",
    "reducer",
    "node",
    "javascript",
    "js"
  ],
  "author": "Abtin Gramian <abtin.gramian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agramian/pdf-compress/issues"
  },
  "homepage": "https://github.com/agramian/pdf-compress#readme",
  "dependencies": {
    "pdfjs-dist": "^2.0.489"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.3.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.0.0-alpha",
    "jasmine-core": "^3.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "pdfjs-dist": "^2.0.489",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "yargs": "^12.0.2"
  }
}
